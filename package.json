{
  "name": "chakrathemes.com",
  "version": "0.4.0",
  "license": "MIT",
  "private": true,
  "description": "A Chakra UI Themes Editor",
  "author": "",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "postinstall": "npm run theme",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "theme": "chakra-cli tokens src/theme/index.ts",
    "theme:watch": "chakra-cli tokens src/theme/index.ts --watch",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:gh": "gatsby build --prefix-paths && gh-pages -d public -b main",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "update-version": "node scripts/update-version.js",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "postversion": "npm run update-version"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^11.18.2",
    "gatsby": "^5.14.1",
    "gatsby-plugin-catch-links": "^5.14.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-responsive-iframe": "^6.14.0",
    "gatsby-remark-smartypants": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "lucide-react": "^0.483.0",
    "node-vibrant": "^4.0.3",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@faker-js/faker": "^9.6.0",
    "@types/chroma-js": "^3.1.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "chroma-js": "^3.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-tsconfig-paths": "^1.0.6",
    "gh-pages": "^6.3.0",
    "prettier": "^3.1.1",
    "typescript": "^5.0.2"
  }
}
